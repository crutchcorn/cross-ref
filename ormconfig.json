{
   "type": "sqlite",
   "database": "./src/data.sqlite",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/entities/**/*.js"
   ],
   "migrations": [
      "src/migration/**/*.js"
   ],
   "subscribers": [
      "src/subscriber/**/*.js"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}
